{
	"compilerOptions": {
		"target": "ES2018",
		"module": "commonjs",
		"jsx": "react",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"*": ["src/server/types/*"]
		},
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"inlineSourceMap": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": ["node_modules"]
}
